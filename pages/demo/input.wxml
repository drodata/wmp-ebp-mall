<import src="../templates/list.wxml"/>
<import src="../templates/input.wxml"/>

<view class="page">
  <view class="page-header">
    <view class="page-title">表单元素</view>
    <view class="page-desc"></view>
  </view>
  <view class="page-body">

    <view class="weui-cells__title">联系人</view>
    <view class="weui-cells weui-cells_after-title">
      <template is="input" data="{{label: 'QQ', name: 'qq'}}"/>
      <template is="input" data="{{label: 'iM', name: 'qq2'}}"/>
      <template is="cell" data="{{...myLocation}}"/>
    </view>

    <view class="weui-cells__title"></view>
    <view class="weui-cells weui-cells_after-title">
      <template is="textarea" data="{{name: 'note', placeholder: '拜访内容', style: 'height: 8em'}}"/>
    </view>


    <view class="weui-btn-area">
        <button bindtap="go" class="weui-btn" type="primary">Navigate</button>
    </view>
  </view>
  <view class="page-footer">
  </view>
</view>

